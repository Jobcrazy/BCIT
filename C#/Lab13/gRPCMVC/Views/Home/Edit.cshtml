@using GrpcServer.Protos
@{
    ViewData["Title"] = "Edit";
    var student =  ViewData["student"] as StudentModel;
}
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div class="form-group">
                <label class="control-label">Student ID</label>
                <input asp-for="@student!.StudentId" class="form-control" />
            </div>

            <div class="form-group">
                <label class="control-label">FirstName</label>
                <input asp-for="@student!.FirstName" class="form-control" />
            </div>

            <div class="form-group">
                <label class="control-label">LastName</label>
                <input asp-for="@student!.LastName" class="form-control" />
            </div>

            <div class="form-group">
                <label class="control-label">School</label>
                <input asp-for="@student!.School" class="form-control" />
            </div>

            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>
